name: branches

on: 
  push:
    branches:
      - develop
    tags: feature

  pull_request:
    branches:
      - develop  
    tags: feature

jobs:
  job1:
    name: First Job
    runs-on: ubuntu-latest
    steps:
      - name: Step One
        uses: actions/checkout@v4
      - name: Step Two
        run: env | sort
  
    
  
